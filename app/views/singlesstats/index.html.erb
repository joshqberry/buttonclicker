<h1>All Singlestats</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>player id</th>
      <th>single id</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @singlesstats.each do |singlestat| %>
      <tr>
        <td><%= singlestat.id %></td>
        <td><%= singlestat.player_id %></td>
        <td><%= singlestat.single_id %></td>

      </tr>

<% end %>
