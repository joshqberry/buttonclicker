<h1>All Doublestats</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>team id</th>
      <th>double id</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @doublesstats.each do |doublestat| %>
      <tr>
        <td><%= doublestat.id %></td>
        <td><%= doublestat.team_id %></td>
        <td><%= doublestat.double_id %></td>

      </tr>

<% end %>
