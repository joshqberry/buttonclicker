<h1>All teams</h1>


<table class="table table-striped">
  <thead>
    <tr>
      <th>Player 1</th>
      <th>Player 2</th>
      <th>Players on team</th>

      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>



    <% @teams.each do |team| %>
      <tr>
        <td><%= team.teammember1 %></td>
        <td><%= team.teammember2 %></td>
        <td><%= link_to team.team_players, team_path(team) %></td>

      </tr>


<% end %>
