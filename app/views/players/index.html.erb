<h1>All players</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Nickname</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @players.each do |player| %>
      <tr>
        <td><%= link_to player.name, player_path(player) %></td>
        <td><%= player.nickname %></td>

      </tr>

<% end %>
