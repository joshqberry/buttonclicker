<div class='page-header'>
  <h1> Edit Singles Game </h1>
</div>


<%= form_for(@single) do |f| %>

<%= f.label :playerA_name %>
<%= f.number_field :playerA_name %>

<%= f.label :playerB_name %>
<%= f.number_field :playerB_name %>

<%= f.label :winner_id %>
<%= f.number_field :winner_id %>

<%= f.label :loser_id %>
<%= f.number_field :loser_id %>

<%= f.label :winner_score %>
<%= f.number_field :winner_score %>

<%= f.label :loser_score %>
<%= f.number_field :loser_score %>

<%= f.submit "Update this singles game" %>

<% end %>
