<h1>All doubles games</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Team A</th>
      <th>Team B</th>
      <th>Winning team</th>
      <th>Losing team</th>
      <th>Winning team score</th>
      <th>Losing team score</th>
      <th>Started at</th>
      <th>Ended at</th>
      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>


    <% @doubles.each do |double| %>
      <tr>
        <td><%= link_to double.teamA_id, team_path(@team) %></td>
        <td><%= link_to double.teamB_id, team_path(@team)  %></td>
        <td><%= double.teamwinner_id %></td>
        <td><%= double.teamloser_id %></td>
        <td><%= double.teamwinner_score %></td>
        <td><%= double.teamloser_score %></td>
        <td><%= double.created_at %></td>
        <td><%= double.ended_at %></td>

      </tr>


<% end %>
